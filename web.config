<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="index.php" />
            </files>
        </defaultDocument>
        <staticContent>
            <mimeMap fileExtension=".json" mimeType="text/plain" />
            <mimeMap fileExtension=".woff" mimeType="application/octet-stream" />
            <mimeMap fileExtension=".woff2" mimeType="application/octet-stream" />
        </staticContent>
        <handlers>
            <remove name="php" />
            <add name="php" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="E:\wamp\bin\php\php5.5.12\php-cgi.exe" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
    </system.webServer>
</configuration>
